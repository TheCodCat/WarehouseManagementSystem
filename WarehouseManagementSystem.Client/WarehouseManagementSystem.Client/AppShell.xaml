<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WarehouseManagementSystem.Client.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WarehouseManagementSystem.Client"
    xmlns:page="clr-namespace:WarehouseManagementSystem.Client.Pages"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Отгрузка партий"
        ContentTemplate="{DataTemplate page:MainPage}"
        Route="MainPage" />

    <ShellContent
    Title="Партии"
    ContentTemplate="{DataTemplate page:PartyViewPage}"
    Route="PartyViewPage"/>

</Shell>
